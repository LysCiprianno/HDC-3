<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sessenta e Seis</title>
    <script>
        function contarValores() {
            let dentroDoIntervalo = 0;
            let foraDoIntervalo = 0;
 
            // Loop para ler 10 valores
            for (let i = 1; i <= 10; i++) {
                let valor = parseInt(prompt("Me informe dez valores: " + i + ":"));
 
                // Verifica se o valor estÃ¡ entre 24 e 42
                if (valor >= 24 && valor <= 42) {
                    dentroDoIntervalo++;
                } else {
                    foraDoIntervalo++;
                }
            }
 
            // Exibe os resultados
            alert("Valores entre 24 e 42: " + dentroDoIntervalo + "\nValores fora do intervalo: " + foraDoIntervalo);
        }
    </script>
</head>
<body onload="contarValores()">
</body>
</html>
